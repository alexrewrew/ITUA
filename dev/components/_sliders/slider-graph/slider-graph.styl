.slider-graph
  margin: 0 -15px;
  &--item
    display flex
    justify-content center
    align-items center
    width 100%
    height 349px
    padding 0 r(15)
    img
      width 100%
  &.slider-with-line.slider-with-line-sync
    .slick-track
      left 0
    .slick-list
      &:before, &:after
        display none


.slider-progressbar.slider-progressbar-graph
  .slider-progressbar--track
    right -65px